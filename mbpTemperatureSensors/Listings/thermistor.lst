N/*----------------------------------------------------------------------------*/
NeMpbError_t vprvTemperatureInterpolation( const xThermistorTemperature_t *pxTemperatureTable,	uint8_t ucLength, float *pfTemperatureCalculated)
N{
N  uint8_t ucStartIndex, usEndIndex, ucGuessIndex;		
N  float fSlope, fTemporalVariable;
N	eMpbError_t	eMpbError;
N	
N	ucStartIndex = 0;
N  usEndIndex = ucLength - 1;
N	
N	/* Validate entry parameter */
N  if( ( pxTemperatureTable == NULL ) ||  ( pfTemperatureCalculated == NULL ) )
X  if( ( pxTemperatureTable == 0 ) ||  ( pfTemperatureCalculated == 0 ) )
N  {
N       return eInvalidParameter;
N  }
N
N	/* Reporting an out of range error when a value exceeds the */
N  /* temperature limits of the temperature table. Temperature */
N  /* is then the maximum or the minimum, accordingly */        
N  if( *pfTemperatureCalculated > (float)pxTemperatureTable[ 0 ].ulResistance )
N  {
N     *pfTemperatureCalculated = pxTemperatureTable[ 0 ].ssTemperature;
N      return eOutOfRange;
N  }
N  else if( *pfTemperatureCalculated < (float)pxTemperatureTable[ ucLength - 1 ].ulResistance )
N  {
N     *pfTemperatureCalculated = pxTemperatureTable[ ucLength - 1 ].ssTemperature;
N     return eOutOfRange;
N  }
N		
N
N	/* binary search for the index of the closest values */
N  do
N  {
N    ucGuessIndex = ( ucStartIndex + usEndIndex ) >> 1;
N
N		if ( ( (float)pxTemperatureTable[ ucGuessIndex ].ulResistance < *pfTemperatureCalculated ) )    
N		{
N        ucStartIndex = ucGuessIndex++;
N		}
N    else
N		{
N        usEndIndex = ucGuessIndex;
N		}
N  } 
N	while ( ( ucStartIndex + 1 ) != usEndIndex);
N	
N
N  /* Perform interpolation */
N	eMpbError = eMpbMathDivisionFloat( ((float)( pxTemperatureTable[ usEndIndex ].ssTemperature - pxTemperatureTable[ ucStartIndex ].ssTemperature )),
N																		 ((float)( pxTemperatureTable[ usEndIndex ].ulResistance - pxTemperatureTable[ ucStartIndex ].ulResistance )), &fSlope);
N	if( eMpbError != eSuccess )
N  {
N        return eMpbError;
N  }																		
N	
N	fTemporalVariable = fSlope * ( *pfTemperatureCalculated -(float)pxTemperatureTable[ ucStartIndex ].ulResistance ) ;
N	
N	*pfTemperatureCalculated = (float)( pxTemperatureTable[ ucStartIndex ].ssTemperature ) + fTemporalVariable ;
N	
N	return eMpbError;
N}
