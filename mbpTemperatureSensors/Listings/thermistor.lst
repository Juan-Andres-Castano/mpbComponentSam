NeMpbError_t eTemperatureAveragingThermistor( handle_t xStream, uint8_t ucLength, const xThermistorTemperature_t *pxTemperatureTable, float *pfTemperatureReaded )
N{
N  
N  static uint8_t 	ucAccumulatorIndex = 0;
N  uint16_t 				usVoltage;	
N	static float 		fTemperatureCalculated = 0.0f;
N  static float		fTemperatureAccumulator = 0.0f; 
N	eMpbError_t			eMpbError;
N	
N		
N  /* Validate parameter */
N  vMpblibsAssertParam( pxTemperatureTable );
X  ;
N		
N	/* Get thermistor voltage */
N	eMpbError = eThermistorAnalogReadmV( xStream, &usVoltage );
N	if( eMpbError != eSuccess )
N	{
N		return eMpbError;
N	}
N	
N	fTemperatureCalculated = (float)usVoltage;
N	
N  /* Interpolate actual raw data to get actual temperature  */
N	eMpbError = vprvTemperatureInterpolation( pxTemperatureTable,	ucLength, &fTemperatureCalculated );
N	
N  /*  Accumulate the temperature measurements */
N   fTemperatureAccumulator += fTemperatureCalculated;
N   ucAccumulatorIndex++;
N
N  /* Compute Average Temperature if enough measurements  */
N  if ( ucAccumulatorIndex >= NUMBER_OF_POINTS_TO_GET_AVERAGE )
X  if ( ucAccumulatorIndex >= 60 )
N  {		
N		eMpbError = eMpbMathDivisionFloat( fTemperatureAccumulator, (float)ucAccumulatorIndex, pfTemperatureReaded );
N		if( eMpbError != eSuccess )
N		{
N			return eMpbError;
N		}
N		fTemperatureAccumulator = 0.0;
N    ucAccumulatorIndex = 0;
N  }
N	
N	return eMpbError;
N}
