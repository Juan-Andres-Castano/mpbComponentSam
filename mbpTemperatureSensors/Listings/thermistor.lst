NeMpbError_t eThermistorGetTemperatureBeta( handle_t xStream, uint32_t ulBetaCoefficient, xThermistorCircuitConfig_t xThermistorCircuitConfig, eThermistorType_t eThermistorType, float *pfTemperature )
N{
N    uint32_t ulThermistance;
N	double dLogResult;
N	float fDivResult;
N	eMpbError_t eMpbError;
N	
N    /* Validate entry parameter */
N	if( pfTemperature == NULL )
X	if( pfTemperature == 0 )
N	{
N		return eInvalidParameter;
N	}
N    
N    /* Get thermistance */
N	eMpbError = eThermistorCircuitGetThermistance( xStream, xThermistorCircuitConfig, &ulThermistance );
N    if( eMpbError != eSuccess )
N    {
N        return eMpbError;
N    }
N    
N    /* log( Rt1/Rt2 )	*/
N	eMpbError = eMpbMathDivisionFloat( eThermistorType, (float)ulThermistance, &fDivResult );
N	if( eMpbError != eSuccess )
N	{
N		return eMpbError;
N	}
N    
N    /* Calculate the log */
N	eMpbError = eThermistorLogarithm( ( double ) fDivResult, &dLogResult );
N	if( eMpbError != eSuccess )
N	{
N		return eMpbError;
N	}	
N    
N	/*	( Beta/T1 ) - log( Rt1/Rt2 )	*/
N	eMpbError = eMpbMathDivisionFloat( (float)ulBetaCoefficient, (float)_25_DEGREES_IN_FAHRENHEIT, &fDivResult );
X	eMpbError = eMpbMathDivisionFloat( (float)ulBetaCoefficient, (float)( 298.15 ), &fDivResult );
N	if( eMpbError != eSuccess )
N	{
N		return eMpbError;
N	}
N	fDivResult -= ( float )dLogResult;	
N	
N    /*	Beta / [ ( Beta/T1 ) - log( Rt1/Rt2 ) ]	*/
N	eMpbError = eMpbMathDivisionFloat( (float)ulBetaCoefficient, (float)fDivResult, &fDivResult );	
N	if( eMpbError != eSuccess )
N	{
N		return eMpbError;
N	}
N    
N	/* conversion from Kelvin to Celsius	*/
N	*pfTemperature = ( fDivResult - (float)KELVIN_TO_DEGREE_CONSTANT );	
X	*pfTemperature = ( fDivResult - (float)( 273.15 ) );	
N	
N	return eMpbError;
N}
